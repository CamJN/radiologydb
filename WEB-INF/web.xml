<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
version="2.4">

	    <servlet>
	        <servlet-name>Login</servlet-name>
	        <servlet-class>loginModule.LoginModule</servlet-class>
	    </servlet>
	    <servlet-mapping>
	        <servlet-name>Login</servlet-name>
	        <url-pattern>/login</url-pattern>
	    </servlet-mapping>

		<servlet>
	        <servlet-name>NewUser</servlet-name>
	        <servlet-class>management.NewUser</servlet-class>
	    </servlet>
	    <servlet-mapping>
	        <servlet-name>NewUser</servlet-name>
	        <url-pattern>/newUser</url-pattern>
	    </servlet-mapping>

        <servlet>
	        <servlet-name>UserUpdate</servlet-name>
	        <servlet-class>management.UserUpdate</servlet-class>
	    </servlet>
	    <servlet-mapping>
	        <servlet-name>UserUpdate</servlet-name>
	        <url-pattern>/userUpdate</url-pattern>
	    </servlet-mapping>


	    <servlet>
                <servlet-name>UploadImage</servlet-name>
                <servlet-class>UploadImage</servlet-class>
            </servlet>
            <servlet-mapping>
                <servlet-name>UploadImage</servlet-name>
                <url-pattern>/UploadImage</url-pattern>
            </servlet-mapping>

	    <servlet>
                <servlet-name>GetOnePic</servlet-name>
                <servlet-class>GetOnePic</servlet-class>
            </servlet>
            <servlet-mapping>
                <servlet-name>GetOnePic</servlet-name>
                <url-pattern>/GetOnePic</url-pattern>
            </servlet-mapping>

	    <servlet>
                <servlet-name>CreateRecord</servlet-name>
                <servlet-class>CreateRecord</servlet-class>
            </servlet>
            <servlet-mapping>
                <servlet-name>CreateRecord</servlet-name>
                <url-pattern>/CreateRecord</url-pattern>
            </servlet-mapping>
        

</web-app>

